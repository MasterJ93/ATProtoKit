ecs/nsid":{"titleInlineContent":[{"text":"NSID Syntax","type":"text"}],"identifier":"https:\/\/atproto.com\/specs\/nsid","type":"link","url":"https:\/\/atproto.com\/specs\/nsid","title":"NSID Syntax"},"https://atproto.com/specs/handle":{"url":"https:\/\/atproto.com\/specs\/handle","identifier":"https:\/\/atproto.com\/specs\/handle","title":"Handle Syntax","type":"link","titleInlineContent":[{"text":"Handle Syntax","type":"text"}]}}}