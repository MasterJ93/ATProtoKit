neContent":[{"text":"The post record that this record is replying to. Optional. Defaults to ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"}],"name":"replyTo"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The embed container attached to the post record. Current items include","type":"text"},{"type":"text","text":" "},{"type":"text","text":"images, videos, external links, records, and post records with media. Optional."},{"type":"text","text":" "},{"type":"text","text":"Defaults to "},{"type":"codeVoice","code":"nil"},{"text":".","type":"text"}]}],"name":"embed"},{"name":"labels","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"An array of labels made by the user. Optional. Defaults to "},{"code":"nil","type":"codeVoice"},{"text":".","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"An array of tags for the post record. Optional. Defaults to ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":"."}]}],"name":"tags"},{"name":"creationDate","content":[{"type":"paragraph","inlineContent":[{"text":"The date of the post record. Defaults to ","type":"text"},{"code":"Date.now","type":"codeVoice"},{"type":"text","text":"."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"The record key of the collection. Optional. Defaults to "},{"code":"nil","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"name":"recordKey"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Indicates whether the record should be validated. Optional."},{"text":" ","type":"text"},{"text":"Defaults to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":".","type":"text"}]}],"name":"shouldValidate"},{"name":"swapCommit","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Swaps out an operation based on the CID. Optional. Defaults to "},{"type":"codeVoice","code":"nil"},{"text":".","type":"text"}]}]}]},{"content":[{"level":2,"type":"heading","anchor":"return-value","text":"Return Value"},{"type":"paragraph","inlineContent":[{"text":"A strong reference, which contains the newly-created record‚Äôs URI and CID hash.","type":"text"}]}],"kind":"content"},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"This can be used instead of creating your own method if you wish not to do so."}],"type":"paragraph"},{"level":1,"text":"Creating a Post","anchor":"Creating-a-Post","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"After you authenticate into Bluesky, you can create a post by using the "},{"type":"codeVoice","code":"text"},{"type":"text","text":" argument:"}]},{"type":"codeListing","code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Just tried out the new coffee shop in town ‚òïÔ∏è ‚Äî highly recommend the cold brew!\"","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift"},{"inlineContent":[{"type":"text","text":"If your post is longer than 300 characters, the method will only take the first 300"},{"text":" ","type":"text"},{"type":"text","text":"characters, discarding the rest."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Bluesky will automatically set the language of the post to English, but if you need to"},{"type":"text","text":" "},{"type":"text","text":"manually set it, you can use the "},{"code":"locales","type":"codeVoice"},{"text":" argument:","type":"text"}]},{"type":"codeListing","code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Premi√®re promenade d'automne üçÇ. Les couleurs sont magnifiques cette ann√©e!\",","        locales: [Locale(identifier: \"fr_FR\")]","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"You can add up to three locales at once; only the first three items will be added as this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"is the limit mandated by Bluesky."}]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"createPostRecord()"},{"text":" will automatically look for URLs, hashtags, and @mentions for you.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"However, if you would like to add inline link facets to your account, you can use the"},{"type":"text","text":" "},{"code":"inlineFacets","type":"codeVoice"},{"type":"text","text":" parameter:"}]},{"syntax":"swift","type":"codeListing","code":["do {","    let inlineFacet = (URL(string: \"https:\/\/www.loc.gov\/resource\/pga.05046\/\"), 57, 72)","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Where's that weird photo of a cat when you need one?\\n\\nAh: here it is!\",","        inlineFacets: [inlineFacet]","    )","","    print(postResult)","} catch {","    throw error","}"]},{"content":[{"inlineContent":[{"text":"","type":"text"},{"code":"startPostion","type":"codeVoice"},{"text":" and ","type":"text"},{"type":"codeVoice","code":"endPostion"},{"type":"text","text":" must be UTF-8 values."}],"type":"paragraph"}],"name":"Note","type":"aside","style":"note"},{"type":"heading","text":"Adding Embedded Content","level":1,"anchor":"Adding-Embedded-Content"},{"type":"paragraph","inlineContent":[{"text":"You can embed various kinds of content in your post, from media to external links,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to other records."}]},{"type":"heading","anchor":"Images","level":2,"text":"Images"},{"type":"paragraph","inlineContent":[{"text":"Use ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoTools\/ImageQuery","type":"reference"},{"text":" to add details to the image, such as","type":"text"},{"type":"text","text":" "},{"text":"alt text, then attach it to the post record.","type":"text"}]},{"code":["do {","    let image = ATProtoTools.ImageQuery(","        imageData: Data(contentsOf: \"\/path\/to\/file\/cat.jpg\"),","        fileName: \"cat.jpg\",","        altText: \"A cat looking annoyed, wearing a hat.\"","    )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I don't think my cat likes her new hat... üôÉ\",","        embed: .images(","            images: [image]","        )","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Up to four images can be attached to a post. All images need to be a .jpg format."}]},{"text":"Videos","type":"heading","level":2,"anchor":"Videos"},{"type":"paragraph","inlineContent":[{"text":"Similar to images, you can add videos to a post.","type":"text"}]},{"code":["let videoURL = URL(string: \"\/path\/to\/file\/beach.mp4\")","","do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"The waves on the beach are looking as beautiful as ever.\",","        embed: .video(video: Data(contentsOf: videoFileURL))","    )","","    print(postResult)","} catch {","    throw error","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"Only one video can be added to a post, can be 100 MB or less, and must use the .mp4 format.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"You can upload up to 25 videos per day and the 25 videos can‚Äôt exceed a total of 500 MB"},{"text":" ","type":"text"},{"text":"for the day.","type":"text"}]},{"style":"tip","name":"Tip","type":"aside","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Bluesky allows you to upload GIFs the same way, as it considers it to be a special kind"},{"type":"text","text":" "},{"type":"text","text":"of video.. The only difference with a GIF is the lack a player scrubber. The same restrictions"},{"text":" ","type":"text"},{"type":"text","text":"with respect to videos apply to uploaded GIFs."}]}]},{"type":"heading","anchor":"External-Links","text":"External Links","level":2},{"type":"paragraph","inlineContent":[{"text":"You can attach a website card to the post.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["Task {","    print(\"Starting application...\")","","    do {","        let session = try await config.authenticate()","","        let externalLinkBuilder = ExternalLinkBuilder()","","        guard let link = URL(string: \"https:\/\/www.youtube.com\/watch?v=dQw4w9WgXcQ\") else { return }","        let metadata = try await linkBuilder.grabMetadata(from: link)","","        let atProto = ATProtoKit(session: session)","        let atProtoBluesky = ATProtoBluesky(atProtoKitInstance: atProto)","","        let postResult = try await atProtoBluesky.createPostRecord(","            text: \"Really glad to hear his talk in person!\",","            embed: .external(","                url: metadata.url,","                title: metadata.title,","                description: metadata.description ?? \"No description given.\",","                thumbnailURL: metadata.thumbnailURL","            )","        )","","        print(\"Post Result: \\(postResult)\")","    } catch {","        throw error","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If there are any links in the post‚Äôs text, the method will not convert it into a"},{"text":" ","type":"text"},{"type":"text","text":"website card; you will need to manually achieve this."}]},{"level":1,"anchor":"Creating-a-Quote-Post","type":"heading","text":"Creating a Quote Post"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Quote posts are also embeds: you simply need to embed the record‚Äôs strong reference to it."}]},{"code":["do {","    let strongReference = ComAtprotoLexicon.Repository.StrongReference(recordURI: record.uri, cidHash: record.cid)","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Me whenever I look at my email:\",","        embed: .record(strongReference: strongReference)","    )","","    print(postResult)","} catch {","    throw error","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Only one record can be embedded. This isn‚Äôt limited to post records, though: you can embed"},{"text":" ","type":"text"},{"text":"any Bluesky-related record that you wish.","type":"text"}]},{"text":"Creating a Reply","type":"heading","anchor":"Creating-a-Reply","level":1},{"type":"paragraph","inlineContent":[{"text":"To create a reply, pass the reply reference of the post‚Äôs reply reference to the post record.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["do {","    let replyReference = try await ATProtoTools().createReplyReference(","        from: parentPost,","        session: session","    )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I also like pancakes.\",","        replyTo: replyReference","    )","","    print(postResult)","} catch {","    throw error","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you want to create a thread of posts, use"},{"text":" ","type":"text"},{"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoTools\/createReplyReference(from:session:)","isActive":true,"type":"reference"},{"type":"text","text":" to create the reply reference and"},{"text":" ","type":"text"},{"text":"pass that over to ","type":"text"},{"code":"replyTo","type":"codeVoice"},{"text":":","type":"text"}]},{"code":["do {","    let threadPost = try await atProtoBluesky.createPostRecord(","        text: \"I like waffles.\"","    )","","    print(\"Replying...\")","","    let firstReplyReference = try await ATProtoTools().createReplyReference(from: threadPost, session: session)","","    let threadPost2 