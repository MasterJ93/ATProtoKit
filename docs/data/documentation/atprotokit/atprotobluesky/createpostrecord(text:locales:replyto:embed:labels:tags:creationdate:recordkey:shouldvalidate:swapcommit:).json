eciseIdentifier":"s:SS","text":"String"},{"text":"? = nil) ","kind":"text"},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"text":"ComAtprotoLexicon","preciseIdentifier":"s:10ATProtoKit17ComAtprotoLexiconV","kind":"typeIdentifier","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Repository","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon\/Repository","preciseIdentifier":"s:10ATProtoKit17ComAtprotoLexiconV10RepositoryV"},{"text":".","kind":"text"},{"kind":"typeIdentifier","text":"StrongReference","preciseIdentifier":"s:10ATProtoKit17ComAtprotoLexiconV10RepositoryV15StrongReferenceV","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon\/Repository\/StrongReference"}]}],"kind":"declarations"},{"parameters":[{"name":"text","content":[{"inlineContent":[{"type":"text","text":"The text that’s directly displayed in the post record. Current limit is"},{"type":"text","text":" "},{"text":"300 characters.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"The languages the text is written in. Current limit is 3 languages.","type":"text"}],"type":"paragraph"}],"name":"locales"},{"name":"replyTo","content":[{"inlineContent":[{"type":"text","text":"The post record that this record is replying to."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The embed container attached to the post record. Current items include"},{"text":" ","type":"text"},{"type":"text","text":"images, videos, external links, records, and post records with media."}]}],"name":"embed"},{"name":"labels","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"An array of labels made by the user."}]}]},{"name":"tags","content":[{"inlineContent":[{"type":"text","text":"An array of tags for the post record."}],"type":"paragraph"}]},{"name":"creationDate","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The date of the post record. Defaults to "},{"code":"Date.now","type":"codeVoice"},{"type":"text","text":"."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"The record key of the collection. Optional."}],"type":"paragraph"}],"name":"recordKey"},{"name":"shouldValidate","content":[{"inlineContent":[{"text":"Indicates whether the record should be validated. Optional.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"Defaults to "},{"type":"codeVoice","code":"true"},{"type":"text","text":"."}],"type":"paragraph"}]},{"name":"swapCommit","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Swaps out an operation based on the CID. Optional."}]}]}],"kind":"parameters"},{"content":[{"anchor":"return-value","text":"Return Value","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A strong reference, which contains the newly-created record’s URI and CID hash."}]}],"kind":"content"},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"This can be used instead of creating your own method if you wish not to do so.","type":"text"}]},{"anchor":"Creating-a-Post","type":"heading","level":2,"text":"Creating a Post"},{"inlineContent":[{"text":"After you authenticate into Bluesky, you can create a post by using the ","type":"text"},{"code":"text","type":"codeVoice"},{"text":" field:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Just tried out the new coffee shop in town ☕️ — highly recommend the cold brew!\"","    )","","    print(postResult)","} catch {","    throw error","}"]},{"inlineContent":[{"text":"If your post is longer than 300 characters, the method will only take the first 300","type":"text"},{"type":"text","text":" "},{"type":"text","text":"characters, discarding the rest."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Bluesky will automatically set the language of the post to English, but if you need to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"manually set it, you can use the "},{"code":"locales","type":"codeVoice"},{"type":"text","text":" argument:"}]},{"code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Première promenade d'automne 🍂. Les couleurs sont magnifiques cette année!\",","        locales: [Locale(identifier: \"fr_FR\")]","    )","","    print(postResult)","} catch {","    throw error","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"You can add up to three locales at once; only the first three items will be added as this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"is the limit mandated by Bluesky."}]},{"anchor":"Adding-Embedded-Content","text":"Adding Embedded Content","level":2,"type":"heading"},{"inlineContent":[{"type":"text","text":"You can embed various kinds of content in your post, from media to external links,"},{"text":" ","type":"text"},{"text":"to other records.","type":"text"}],"type":"paragraph"},{"type":"heading","level":3,"anchor":"Images","text":"Images"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use "},{"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoTools\/ImageQuery","isActive":true,"type":"reference"},{"text":" to add details to the image, such as","type":"text"},{"text":" ","type":"text"},{"text":"alt text, then attach it to the post record.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["do {","    let image = ATProtoTools.ImageQuery(","                    imageData: Data(contentsOf: \"\/path\/to\/file\/cat.jpg\"),","                    fileName: \"cat.jpg\",","                    altText: \"A cat looking annoyed, waring a hat.\"","                )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I don't think my cat likes her new hat... 🙃\",","        embed: .images(","            images: [image]","        )","    )","","    print(postResult)","} catch {","    throw error","}"]},{"type":"paragraph","inlineContent":[{"text":"Up to four images can be attached to a post. All images need to be a .jpg format.","type":"text"}]},{"level":3,"text":"Videos","type":"heading","anchor":"Videos"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Similar to images, you can add videos to a post."}]},{"type":"codeListing","syntax":"swift","code":["let videoURL = URL(string: \"\/path\/to\/file\/beach.mp4\")","","do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"The waves on the beach are looking as beautiful as ever.\",","        embed: .video(video: Data(contentsOf: videoFileURL))","    )","","    print(postResult)","} catch {","    throw error","}"]},{"type":"paragraph","inlineContent":[{"text":"Only one video can be added to a post, can be 50 MB or less, and must use the .mp4 format.","type":"text"},{"text":" ","type":"text"},{"text":"You can upload up to 25 videos per day and the 25 videos can’t exceed a total of 500 MB","type":"text"},{"type":"text","text":" "},{"type":"text","text":"for the day."}]},{"level":3,"anchor":"External-Links","text":"External Links","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can attach a website card to the post."}]},{"code":["Task {","    print(\"Starting application...\")","","    do {","        let session = try await config.authenticate()","","        let externalLinkBuilder = ExternalLinkBuilder()","","        guard let link = URL(string: \"https:\/\/www.youtube.com\/watch?v=dQw4w9WgXcQ\") else { return }","        let metadata = try await linkBuilder.grabMetadata(from: link)","","        let atProto = ATProtoKit(session: session)","        let atProtoBluesky = ATProtoBluesky(atProtoKitInstance: atProto)","","        let postResult = try await atProtoBluesky.createPostRecord(","            text: \"Really glad to hear his talk in person!\",","            embed: .external(","                url: metadata.url,","                title: metadata.title,","                description: metadata.description ?? \"No description given.\",","                thumbnailURL: metadata.thumbnailURL","            )","        )","","        print(\"Post Result: \\(postResult)\")","    } catch {","        throw error","    }","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"If there are any links in the post’s text, the method will not convert it into a"},{"type":"text","text":" "},{"text":"website card; you will need to manually achieve this.","type":"text"}],"type":"paragraph"},{"text":"Creating a Quote Post","level":2,"type":"heading","anchor":"Creating-a-Quote-Post"},{"inlineContent":[{"text":"Quote posts are also embeds: you simply need to embed the record’s strong reference to it.","type":"text"}],"type":"paragraph"},{"code":["do {","    let strongReference = ComAtprotoLexicon.Repository.StrongReference(recordURI: record.uri, cidHash: record.cid)","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Me whenever I look at my email:\",","        embed: .record(strongReference: strongReference)","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Only one record can be embedded. This isn’t limited to post records, though: you can embed"},{"text":" ","type":"text"},{"text":"any Bluesky-related record that you wish.","type":"text"}]},{"type":"heading","text":"Creating a Reply","anchor":"Creating-a-Reply","level":2},{"type":"paragraph","inlineContent":[{"text":"To create a reply, pass the reply reference of the post’s reply reference to the post record.","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["do {","    let replyReference = try await ATProtoTools().createReplyReference(","        from: parentPost,","        session: session","    )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I also like pancakes.\",","        replyTo: replyReference","    )","","    print(postResult)","} catch {","    throw error","}"]},{"inlineContent":[{"type":"text","text":"If you want to create a thread of posts, use"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoTools\/createReplyReference(from:session:)","isActive":true},{"type":"text","text":" to create the reply reference and"},{"text":" ","type":"text"},{"type":"text","text":"pass that over to "},{"type":"codeVoice","code":"replyTo"},{"type":"text","text":":"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["do {","    let threadPost = try await atProtoBluesky.createPostRecord(","        text: \"I like waffles.\"","    )","","    print(\"Replying...\")","","    let firstReplyReference = try await ATProtoTools().createReplyReference(from: threadPost, session: session)","","    let threadPost2 = try await atProtoBluesky.createPostRecord(","        text: \"I also like pancakes.\",","        replyTo: firstReplyReference","    )","","    print(\"Second reply...\")","","    let secondReplyReference = try await ATProtoTools().createReplyReference(from: threadPost2, session: session)","","    let threadPost3 = try await atProtoBluesky.createPostRecord(","        text: \"I also like ice cream.\",","        replyTo: secondReplyReference","    )","} catch {","    throw error","}"]}]}],"kind":"symbol","identifier":{"interfaceLanguage":"swift","url":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoBluesky\/createPostRecord(text:locales:replyTo:embed:labels:tags:creationDate:re