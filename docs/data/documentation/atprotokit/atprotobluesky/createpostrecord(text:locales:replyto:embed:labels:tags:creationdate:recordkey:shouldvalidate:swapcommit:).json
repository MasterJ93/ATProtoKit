xt":"Repository","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon\/Repository","kind":"typeIdentifier","preciseIdentifier":"s:10ATProtoKit17ComAtprotoLexiconV10RepositoryV"},{"text":".","kind":"text"},{"preciseIdentifier":"s:10ATProtoKit17ComAtprotoLexiconV10RepositoryV15StrongReferenceV","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon\/Repository\/StrongReference","kind":"typeIdentifier","text":"StrongReference"}],"languages":["swift"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The text thatâ€™s directly displayed in the post record. Current limit is"},{"type":"text","text":" "},{"type":"text","text":"300 characters."}]}],"name":"text"},{"name":"locales","content":[{"inlineContent":[{"text":"The languages the text is written in. Current limit is 3 languages.","type":"text"}],"type":"paragraph"}]},{"name":"replyTo","content":[{"type":"paragraph","inlineContent":[{"text":"The post record that this record is replying to.","type":"text"}]}]},{"content":[{"inlineContent":[{"text":"The embed container attached to the post record. Current items include","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"images, videos, external links, records, and post records with media."}],"type":"paragraph"}],"name":"embed"},{"name":"labels","content":[{"inlineContent":[{"type":"text","text":"An array of labels made by the user."}],"type":"paragraph"}]},{"name":"tags","content":[{"inlineContent":[{"text":"An array of tags for the post record.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The date of the post record. Defaults to "},{"type":"codeVoice","code":"Date.now"},{"type":"text","text":"."}]}],"name":"creationDate"},{"name":"recordKey","content":[{"inlineContent":[{"type":"text","text":"The record key of the collection. Optional."}],"type":"paragraph"}]},{"name":"shouldValidate","content":[{"inlineContent":[{"type":"text","text":"Indicates whether the record should be validated. Optional."},{"text":" ","type":"text"},{"type":"text","text":"Defaults to "},{"type":"codeVoice","code":"true"},{"type":"text","text":"."}],"type":"paragraph"}]},{"name":"swapCommit","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Swaps out an operation based on the CID. Optional."}]}]}],"kind":"parameters"},{"kind":"content","content":[{"text":"Return Value","type":"heading","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A strong reference, which contains the newly-created recordâ€™s URI and CID hash.","type":"text"}]}]},{"kind":"content","content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This can be used instead of creating your own method if you wish not to do so."}]},{"text":"Creating a Post","level":2,"anchor":"Creating-a-Post","type":"heading"},{"inlineContent":[{"text":"After you authenticate into Bluesky, you can create a post by using the ","type":"text"},{"type":"codeVoice","code":"text"},{"type":"text","text":" field:"}],"type":"paragraph"},{"code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Just tried out the new coffee shop in town â˜•ï¸ â€” highly recommend the cold brew!\"","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift","type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"If your post is longer than 300 characters, the method will only take the first 300","type":"text"},{"type":"text","text":" "},{"type":"text","text":"characters, discarding the rest."}]},{"inlineContent":[{"text":"Bluesky will automatically set the language of the post to English, but if you need to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"manually set it, you can use the "},{"type":"codeVoice","code":"locales"},{"type":"text","text":" argument:"}],"type":"paragraph"},{"type":"codeListing","code":["do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"PremiÃ¨re promenade d'automne ðŸ‚. Les couleurs sont magnifiques cette annÃ©e!\",","        locales: [Locale(identifier: \"fr_FR\")]","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can add up to three locales at once; only the first three items will be added as this"},{"type":"text","text":" "},{"type":"text","text":"is the limit mandated by Bluesky."}]},{"level":2,"type":"heading","anchor":"Adding-Embedded-Content","text":"Adding Embedded Content"},{"type":"paragraph","inlineContent":[{"text":"You can embed various kinds of content in your post, from media to external links,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to other records."}]},{"type":"heading","text":"Images","level":3,"anchor":"Images"},{"inlineContent":[{"type":"text","text":"Use "},{"type":"reference","isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ComAtprotoLexicon\/Repository\/ImageQuery"},{"text":" to add details to the image, such as","type":"text"},{"type":"text","text":" "},{"text":"alt text, then attach it to the post record.","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["do {","    let image = ComAtprotoLexicon.Repository.ImageQuery(","                    imageData: Data(contentsOf: \"\/path\/to\/file\/cat.jpg\"),","                    fileName: \"cat.jpg\",","                    altText: \"A cat looking annoyed, waring a hat.\"","                )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I don't think my cat likes her new hat... ðŸ™ƒ\",","        embed: .images(","            images: [image]","        )","    )","","    print(postResult)","} catch {","    throw error","}"]},{"inlineContent":[{"text":"Up to four images can be attached to a post. All images need to be a .jpg format.","type":"text"}],"type":"paragraph"},{"level":3,"anchor":"Videos","text":"Videos","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Similar to images, you can add videos to a post.","type":"text"}]},{"type":"codeListing","code":["let videoURL = URL(string: \"\/path\/to\/file\/beach.mp4\")","","do {","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"The waves on the beach are looking as beautiful as ever.\",","        embed: .video(video: Data(contentsOf: videoFileURL))","    )","","    print(postResult)","} catch {","    throw error","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Only one video can be added to a post, can be 50 MB or less, and must use the .mp4 format."},{"text":" ","type":"text"},{"text":"You can upload up to 25 videos per day and the 25 videos canâ€™t exceed a total of 500 MB","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"for the day."}]},{"level":3,"type":"heading","anchor":"External-Links","text":"External Links"},{"inlineContent":[{"type":"text","text":"You can attach a website card to the post."}],"type":"paragraph"},{"code":["Task {","    print(\"Starting application...\")","","    do {","        let session = try await config.authenticate()","","        let externalLinkBuilder = ExternalLinkBuilder()","","        guard let link = URL(string: \"https:\/\/www.youtube.com\/watch?v=dQw4w9WgXcQ\") else { return }","        let metadata = try await linkBuilder.grabMetadata(from: link)","","        let atProto = ATProtoKit(session: session)","        let atProtoBluesky = ATProtoBluesky(atProtoKitInstance: atProto)","","        let postResult = try await atProtoBluesky.createPostRecord(","            text: \"Really glad to hear his talk in person!\",","            embed: .external(","                url: metadata.url,","                title: metadata.title,","                description: metadata.description ?? \"No description given.\",","                thumbnailURL: metadata.thumbnailURL","            )","        )","","        print(\"Post Result: \\(postResult)\")","    } catch {","        throw error","    }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"If there are any links in the postâ€™s text, the method will not convert it into a","type":"text"},{"type":"text","text":" "},{"text":"website card; you will need to manually achieve this.","type":"text"}]},{"level":2,"type":"heading","anchor":"Creating-a-Quote-Post","text":"Creating a Quote Post"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Quote posts are also embeds: you simply need to embed the recordâ€™s strong reference to it."}]},{"syntax":"swift","code":["do {","    let strongReference = ComAtprotoLexicon.Repository.StrongReference(recordURI: record.uri, cidHash: record.cid)","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"Me whenever I look at my email:\",","        embed: .record(strongReference: strongReference)","    )","","    print(postResult)","} catch {","    throw error","}"],"type":"codeListing"},{"inlineContent":[{"text":"Only one record can be embedded. This isnâ€™t limited to post records, though: you can embed","type":"text"},{"type":"text","text":" "},{"type":"text","text":"any Bluesky-related record that you wish."}],"type":"paragraph"},{"text":"Creating a Reply","level":2,"anchor":"Creating-a-Reply","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"To create a reply, pass the reply reference of the postâ€™s reply reference to the post record."}]},{"syntax":"swift","code":["do {","    let replyReference = try await ATProtoTools().createReplyReference(","        from: parentPost,","        session: session","    )","","    let postResult = try await atProtoBluesky.createPostRecord(","        text: \"I also like pancakes.\",","        replyTo: replyReference","    )","","    print(postResult)","} catch {","    throw error","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you want to create a thread of posts, use"},{"type":"text","text":" "},{"isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoTools\/createReplyReference(from:session:)","type":"reference"},{"text":" to create the reply reference and","type":"text"},{"type":"text","text":" "},{"text":"pass that over to ","type":"text"},{"code":"replyTo","type":"codeVoice"},{"type":"text","text":":"}]},{"type":"codeListing","syntax":"swift","code":["do {","    let threadPost = try await atProtoBluesky.createPostRecord(","        text: \"I like waffles.\"","    )","","    print(\"Replying...\")","","    let firstReplyReference = try await ATProtoTools().createReplyReference(from: threadPost, session: session)","","    let threadPost2 = try await atProtoBluesky.createPostRecord(","        text: \"I also like pancakes.\",","        replyTo: firstReplyReference","    )","","    print(\"Second reply...\")","","    let secondReplyReference = try await ATProtoTools().createReplyReference(from: threadPost2, session: session)","","    let threadPost3 = try await atProtoBluesky.createPostRecord(","        text: \"I also like ice cream.\",","        replyTo: secondReplyReference","    )","} catch {","    throw error","}"]}]}],"metadata":{"extendedModule":"ATProtoKit","modules":[{"name":"ATProtoKit"}],"fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"createPostRecord","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"text"},{"text":": ","kind":"text"},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":", ","kind":"text"},{"kind":"externalParam","text":"locales"},{"kind":"text","text":": ["},{"preciseIdentifier":"s:10Foundation6LocaleV","kind":"typeIdentifier","text":"Locale"},{"kind":"text","t