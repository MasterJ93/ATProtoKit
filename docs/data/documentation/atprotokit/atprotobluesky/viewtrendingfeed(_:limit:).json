ragraph"}],"type":"aside","style":"note","name":"Note"},{"inlineContent":[{"type":"text","text":"If there’s a "},{"type":"codeVoice","code":"cursor"},{"text":" present, use the ","type":"text"},{"code":"getFeed","type":"codeVoice"},{"type":"text","text":" method to pass in the method’s "},{"code":"atURI","type":"codeVoice"},{"type":"text","text":" and "},{"code":"feed","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"variables from the output’s tuple."}],"type":"paragraph"},{"code":["do {","    \/\/ Continued from the first example...","    let atURI = trendingFeedResult.atURI","    let cursor = trendingFeedResult.posts.cursor","","    let getFeedContinuedResults = try await atProtoKit.getFeed(by: atURI, cursor: cursor)","    print(\"Feed with cursor: \\(getFeedContinuedResults)\")","} catch {","    throw error","}"],"syntax":"swift","type":"codeListing"},{"style":"note","name":"Throws","type":"aside","content":[{"inlineContent":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATProtoError","type":"reference"},{"type":"text","text":"-conforming error type, depending on the issue. Go to"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATAPIError"},{"text":" and ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATRequestPrepareError"},{"type":"text","text":" for more details."}],"type":"paragraph"}]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/atprotokit\/atprotobluesky\/viewtrendingfeed(_:limit:)"],"traits":[{"interfaceLanguage":"swift"}]}],"kind":"symbol","references":{"atprotokit_icon.png":{"alt":"A technology icon representing the ATProtoKit framework.","variants":[{"url":"\/images\/atprotokit_icon.png","traits":["1x","light"]}],"identifier":"atprotokit_icon.png","type":"image"},"doc://ATProtoKit/documentation/ATProtoKit/AppBskyLex